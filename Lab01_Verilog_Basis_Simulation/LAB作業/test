    %mov 8, 4, 1;
    %load/v 9, v00F41300_0, 4;
    %mov 13, 0, 2;
    %cmpi/u 9, 15, 6;
    %mov 9, 4, 1;
    %and 8, 9, 1;
    %jmp/0xz  T_3.4, 8;
    %load/v 8, v00F41358_0, 4;
    %mov 12, 0, 28;
    %addi 8, 1, 32;

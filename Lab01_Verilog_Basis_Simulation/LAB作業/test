    .scope S_01068200;
T_3 ;
    %wait E_0106A748;
    %load/v 8, v00F41300_0, 4;
    %mov 12, 0, 2;
   %cmpi/u 8, 15, 6;
    %or 5, 4, 1;
    %jmp/0xz  T_3.0, 5;
    %load/v 8, v00F41300_0, 4;
    %mov 12, 0, 28;
    %addi 8, 1, 32;
    %ix/load 0, 4, 0;
    %assign/v0 v00F41300_0, 0, 8;
T_3.0 ;
    %load/v 8, v00F41300_0, 4;
    %mov 12, 0, 2;

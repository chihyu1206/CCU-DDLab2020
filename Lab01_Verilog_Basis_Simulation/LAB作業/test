    .scope S_01068200;
T_4 ;
    %vpi_call 2 43 "$monitor", "%4dns monitor: a=%d b=%d c=%d d=%d sum=%d ov=%d", $stime, v00F41300_0, v00F41358_0, v00F410F0_0, v00F40CD0_0, v00F40E88_0, v00F40D28_0;
    %end;

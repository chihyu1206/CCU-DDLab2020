S_00FB7560 .scope module, "tb_lab9" "tb_lab9" 2 5;
 .timescale -9 -12;
v00FBAFA0_0 .var "CLK", 0 0;
v00FBAB18_0 .var "correct_ans", 15 0;
v00FBAB70_0 .var "count", 5 0;
v00FBABC8_0 .var "error", 0 0;
v00FF12B0_0 .var "in_a", 7 0;

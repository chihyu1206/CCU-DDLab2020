v00FF14C0_0 .net "out_valid", 0 0, v00FE5DD8_0; 1 drivers
v00FF1570_0 .var "reset", 0 0;
v00FF1308_0 .var "temp_a", 7 0;
v00FF1780_0 .var "temp_b", 7 0;
S_00FB7CD0 .scope module, "m1" "lab9" 2 23, 3 5, S_00FB7560;
 .timescale -9 -12;
v00FBA728_0 .net "CLK", 0 0, v00FBAFA0_0; 1 drivers
v00FB8BB0_0 .var "Counter", 5 0;
v00FB9818_0 .var "Mplicand", 15 0;
v00FBA5A8_0 .var "Mplier", 7 0;
v00FBA600_0 .var "Product", 15 0;
v00FE5DD8_0 .var "Product_Valid", 0 0;
v00FE5E30_0 .net "RST", 0 0, v00FF1570_0; 1 drivers
v00FBAEF0_0 .net "in_a", 7 0, v00FF12B0_0; 1 drivers
v00FBAF48_0 .net "in_b", 7 0, v00FF15C8_0; 1 drivers
E_00FE4E28 .event posedge, v00FE5E30_0, v00FBA728_0;
    .scope S_00FB7CD0;
T_0 ;
    %wait E_00FE4E28;
    %load/v 8, v00FE5E30_0, 1;
    %jmp/0xz  T_0.0, 8;
    %ix/load 0, 6, 0;
    %assign/v0 v00FB8BB0_0, 0, 0;
    %jmp T_0.1;
T_0.0 ;
    %ix/load 0, 1, 0;
    %load/vp0 8, v00FB8BB0_0, 6;
    %ix/load 0, 6, 0;
    %assign/v0 v00FB8BB0_0, 0, 8;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_00FB7CD0;
T_1 ;
    %wait E_00FE4E28;
    %load/v 8, v00FE5E30_0, 1;
    %jmp/0xz  T_1.0, 8;
    %ix/load 0, 16, 0;
    %assign/v0 v00FBA600_0, 0, 0;
    %ix/load 0, 16, 0;
    %assign/v0 v00FB9818_0, 0, 0;
    %ix/load 0, 8, 0;
    %assign/v0 v00FBA5A8_0, 0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/v 8, v00FB8BB0_0, 6;
    %cmpi/u 8, 0, 6;
    %jmp/0xz  T_1.2, 4;
    %ix/load 0, 16, 0;
    %assign/v0 v00FBA600_0, 0, 0;
    %load/v 8, v00FBAEF0_0, 8;
    %mov 16, 0, 8;
    %ix/load 0, 16, 0;
    %assign/v0 v00FB9818_0, 0, 8;
    %load/v 8, v00FBAF48_0, 8;
    %ix/load 0, 8, 0;
    %assign/v0 v00FBA5A8_0, 0, 8;
    %jmp T_1.3;
T_1.2 ;
    %load/v 8, v00FB8BB0_0, 6;
   %cmpi/u 8, 8, 6;
    %or 5, 4, 1;
    %jmp/0xz  T_1.4, 5;
    %load/v 8, v00FBA5A8_0, 1; Only need 1 of 8 bits
; Save base=8 wid=1 in lookaside.
    %jmp/0xz  T_1.6, 8;
    %load/v 8, v00FB9818_0, 16;
    %load/v 24, v00FBA600_0, 16;
    %add 8, 24, 16;
    %ix/load 0, 16, 0;
    %assign/v0 v00FBA600_0, 0, 8;
T_1.6 ;
    %load/v 8, v00FB9818_0, 16;
    %ix/load 0, 1, 0;
    %mov 4, 0, 1;
    %shiftl/i0  8, 16;
    %ix/load 0, 16, 0;
    %assign/v0 v00FB9818_0, 0, 8;
    %load/v 8, v00FBA5A8_0, 8;
    %ix/load 0, 1, 0;
    %mov 4, 0, 1;
    %shiftr/i0  8, 8;
    %ix/load 0, 8, 0;
    %assign/v0 v00FBA5A8_0, 0, 8;
    %jmp T_1.5;
T_1.4 ;
    %load/v 8, v00FBA600_0, 16;
    %ix/load 0, 16, 0;
    %assign/v0 v00FBA600_0, 0, 8;
    %load/v 8, v00FB9818_0, 16;
    %ix/load 0, 16, 0;
    %assign/v0 v00FB9818_0, 0, 8;
    %load/v 8, v00FBA5A8_0, 8;
    %ix/load 0, 8, 0;
    %assign/v0 v00FBA5A8_0, 0, 8;
T_1.5 ;
T_1.3 ;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_00FB7CD0;
T_2 ;
    %wait E_00FE4E28;
    %load/v 8, v00FE5E30_0, 1;
    %jmp/0xz  T_2.0, 8;
    %ix/load 0, 1, 0;
    %assign/v0 v00FE5DD8_0, 0, 0;
    %jmp T_2.1;
T_2.0 ;
    %load/v 8, v00FB8BB0_0, 6;
    %cmpi/u 8, 9, 6;
    %jmp/0xz  T_2.2, 4;
    %ix/load 0, 1, 0;
    %assign/v0 v00FE5DD8_0, 0, 1;
    %jmp T_2.3;
T_2.2 ;
    %ix/load 0, 1, 0;
    %assign/v0 v00FE5DD8_0, 0, 0;
T_2.3 ;
T_2.1 ;
    %jmp T_2;
    .thread T_2;
    .scope S_00FB7560;
T_3 ;
    %vpi_call 2 27 "$dumpfile", "lab9_ex.vcd";
    %vpi_call 2 28 "$dumpvars";
    %set/v v00FBAFA0_0, 0, 1;
    %delay 20000, 0;
    %set/v v00FF1570_0, 1, 1;
    %movi 8, 3, 8;
    %set/v v00FF1308_0, 8, 8;
    %movi 8, 9, 8;
    %set/v v00FF1780_0, 8, 8;
    %delay 20000, 0;
    %set/v v00FF1570_0, 0, 1;
    %delay 700000, 0;
    %set/v v00FF1570_0, 1, 1;
    %movi 8, 111, 8;
    %set/v v00FF1308_0, 8, 8;
    %movi 8, 101, 8;
    %set/v v00FF1780_0, 8, 8;
    %delay 20000, 0;
    %set/v v00FF1570_0, 0, 1;
    %delay 700000, 0;
    %set/v v00FF1570_0, 1, 1;
    %movi 8, 10, 8;
    %set/v v00FF1308_0, 8, 8;
    %movi 8, 14, 8;
    %set/v v00FF1780_0, 8, 8;
    %delay 20000, 0;
    %set/v v00FF1570_0, 0, 1;
    %delay 700000, 0;
    %set/v v00FF1570_0, 1, 1;
    %movi 8, 123, 8;
    %set/v v00FF1308_0, 8, 8;
    %movi 8, 7, 8;
    %set/v v00FF1780_0, 8, 8;
    %delay 20000, 0;
    %set/v v00FF1570_0, 0, 1;
    %delay 700000, 0;
    %set/v v00FF1570_0, 1, 1;
    %movi 8, 1, 8;
    %set/v v00FF1308_0, 8, 8;
    %movi 8, 60, 8;
    %set/v v00FF1780_0, 8, 8;
    %delay 20000, 0;
    %set/v v00FF1570_0, 0, 1;
    %delay 20000, 0;
    %set/v v00FF1570_0, 0, 1;
    %delay 700000, 0;
    %vpi_call 2 59 "$finish";
    %end;
    .thread T_3;
    .scope S_00FB7560;
T_4 ;
    %delay 10000, 0;
    %load/v 8, v00FBAFA0_0, 1;
    %inv 8, 1;
    %set/v v00FBAFA0_0, 8, 1;
    %jmp T_4;
    .thread T_4;
    .scope S_00FB7560;
T_5 ;
    %wait E_00FE4E28;
    %load/v 8, v00FF1570_0, 1;
    %jmp/0xz  T_5.0, 8;
    %ix/load 0, 6, 0;
    %assign/v0 v00FBAB70_0, 0, 0;
    %load/v 8, v00FF1308_0, 8;
    %ix/load 0, 8, 0;
    %assign/v0 v00FF12B0_0, 0, 8;
    %load/v 8, v00FF1780_0, 8;
    %ix/load 0, 8, 0;
    %assign/v0 v00FF15C8_0, 0, 8;
    %ix/load 0, 16, 0;
    %assign/v0 v00FBAB18_0, 0, 0;
    %ix/load 0, 1, 0;
    %assign/v0 v00FBABC8_0, 0, 0;
    %jmp T_5.1;
T_5.0 ;
    %load/v 8, v00FF12B0_0, 8;
    %movi 16, 0, 8;
    %load/v 24, v00FF15C8_0, 8;
    %movi 32, 0, 8;
    %mul 8, 24, 16;
    %ix/load 0, 16, 0;
    %assign/v0 v00FBAB18_0, 0, 8;
    %load/v 8, v00FBAB70_0, 6;
    %mov 14, 0, 26;
    %addi 8, 1, 32;
    %ix/load 0, 6, 0;
    %assign/v0 v00FBAB70_0, 0, 8;
    %load/v 8, v00FF14C0_0, 1;
    %mov 9, 0, 2;
    %cmpi/u 8, 1, 3;
    %jmp/0xz  T_5.2, 4;
    %load/v 8, v00FF1620_0, 16;
    %load/v 24, v00FBAB18_0, 16;
    %cmp/u 8, 24, 16;
    %inv 4, 1;
    %jmp/0xz  T_5.4, 4;
    %ix/load 0, 1, 0;
    %assign/v0 v00FBABC8_0, 0, 1;
    %vpi_call 2 86 "$display";
    %vpi_call 2 87 "$display", "//////////";
    %vpi_call 2 88 "$display", "// Fail //";
    %vpi_call 2 89 "$display", "//////////";
    %vpi_call 2 90 "$display", "doing %d * %d ...\012", v00FF1308_0, v00FF1780_0;
    %vpi_call 2 91 "$display", "your answer is %d,\012but correct answer is %d\012", v00FF1620_0, v00FBAB18_0;
    %vpi_call 2 93 "$display";
    %jmp T_5.5;
T_5.4 ;
    %ix/load 0, 1, 0;
    %assign/v0 v00FBABC8_0, 0, 0;
    %vpi_call 2 98 "$display";
    %vpi_call 2 99 "$display", "////////////////";
    %vpi_call 2 100 "$display", "// Successful //";
    %vpi_call 2 101 "$display", "////////////////";
    %vpi_call 2 102 "$display", "doing %d * %d ...\012", v00FF1308_0, v00FF1780_0;
    %vpi_call 2 103 "$display", "your answer is %d,\012correct answer is %d\012", v00FF1620_0, v00FBAB18_0;
    %vpi_call 2 105 "$display";
T_5.5 ;
T_5.2 ;
T_5.1 ;
    %jmp T_5;
    .thread T_5;
